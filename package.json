{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": " npx eslint App.tsx && npx eslint ./src/**/* --no-error-on-unmatched-pattern && npx typescript -p ./tsconfig.json",
    "jest": "node_modules/.bin/jest",
    "jest:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "test": "npm run lint && npm run jest"
  },
  "jest": {
    "preset": "jest-expo-enzyme",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
  },
  "dependencies": {
    "expo": "~36.0.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-web": "~0.11.7",
    "react-navigation": "^4.3.1",
    "react-navigation-drawer": "^2.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "^16.9.0",
    "@types/react-native": "^0.60.23",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-preset-expo": "~8.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^3.6.3",
    "eslint-plugin-react-native": "^3.7.0",
    "jest-expo": "^36.0.1",
    "jest-expo-enzyme": "^1.0.0",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.1",
    "typescript": "^3.6.3"
  },
  "private": true
}
